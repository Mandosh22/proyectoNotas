
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<h2>Profesores</h2>
<form id="frmLogin">
    
    <div class="form-group">
        <label>UserName</label>
        <input name="UserName" id="UserName" type="text" placeholder="Usuario" class="form-control" required />

    </div>
    <br />
    <br />
    <div class="form-group">
        <label>Contraseña</label>
        <input name="Contraseña" id="Contraseña" type="password" placeholder="Contraseña" class="form-control" required />
    </div>

    <div class="form-group">

        <input type="submit" id="btnEnviar" value="Entrar" class="btn btn-primary " />

    </div>
</form>


<br />
<br />

<br />
<br />
<h2>Administradores</h2>
<form id="frmLoginAdmi">

    <div class="form-group">
        <label>UserName</label>
        <input name="UserNameAdmi" id="UserNameAdmi" type="text" placeholder="Usuario" class="form-control" required />

    </div>
    <br />
    <br />
    <div class="form-group">
        <label>Contraseña</label>
        <input name="ContraseñaAdmi" id="ContraseñaAdmi" type="password" placeholder="Contraseña" class="form-control" required />
    </div>

    <div class="form-group">

        <input type="submit" id="btnEnviar2" value="Entrar" class="btn btn-primary " />

    </div>
</form>



